@{
    ViewBag.Title = "Registration Failed";
}

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
</hgroup>
<div>
    <p>
        There was a problem processing your registration.
        <br/>
        Please click @Html.ActionLink("here", "Register", "Account", null, new {id = "registerLink"}) to try again.
    </p>
</div>